    "version": "0.2.0",
    "configurations": [
    

        {
            "name": "Python: Flask",
            "type": "debugpy",
            "request": "launch",
            "program": "frontend/flaskmiddle/run.py",
            "jinja": true,
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: QUERIES Celery WINDOWS",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.worker.celery_start_queries",
                "worker",
                "--pool=threads",
                "--concurrency=8",
                "-Q",
                "fila_queries",
                "-l",
                "INFO",
                "-n",
                "worker1@%h"
            ],
            "jinja": true,
            "justMyCode": true,
            "gevent": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: QUERIES Celery LINUX",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.worker.celery_start_queries",
                "worker",
                "--pool=gevent",
                "-c",
                "4",
                "-Q",
                "fila_queries",
                "-l",
                "INFO",
                "-n",
                "worker1@%h"
            ],
            "jinja": true,
            "justMyCode": true,
            "gevent": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: INDICADORES gRPC SERVERPPG",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api.py",  
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        {
            "name": "Python: INDICADORES gRPC SERVERPPGLS",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api_ppgls.py",  
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
    ]

}
