    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "debugpy",
            "request": "launch",
            "program": "frontend/flaskmiddle/run.py",
            "jinja": true,
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: QUERIES Celery WINDOWS",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.worker.celery_start_queries",
                "worker",
                "--pool=threads",
                "--concurrency=8",
                "-Q",
                "fila_queries",
                "-l",
                "INFO",
                "-n",
                "worker1@%h",
                "--events"
            ],
            "jinja": true,
            "justMyCode": true,
            "gevent": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: QUERIES Celery LINUX",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.worker.celery_start_queries",
                "worker",
                "--pool=gevent",
                "-c",
                "4",
                "-Q",
                "fila_queries",
                "-l",
                "INFO",
                "-n",
                "worker1@%h"
            ],
            "jinja": true,
            "justMyCode": true,
            "gevent": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: SERVER PPG",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        {
            "name": "Python: SERVER PPGLS",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api_ppgls.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        {
            "name": "Python: SERVER GRAD",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api_grad.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        {
            "name": "Python: SERVER USUARIOS",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api_usuarios.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        
	{
	      "name": "Next.js: debug full stack",
	      "type": "node-terminal",
	      "cwd": "${workspaceFolder}/frontend/frontend-next",
	      "request": "launch",
	      "command": "npm run dev",
	      "serverReadyAction": {
		"pattern": "- Local:.+(https?://.+)",
		"uriFormat": "%s",
		"action": "debugWithChrome"
	      }
    }	
    ]
