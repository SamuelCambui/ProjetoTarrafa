{
    // Use o IntelliSense para saber mais sobre os atributos possíveis.
    // Focalizar para exibir as descrições dos atributos existentes.
    // Para obter mais informações, acesse: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "backend.app.api.api_login:app",
                "--reload",
                "--port",
                "8002"
            ],
            "jinja": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: Flask",
            "type": "debugpy",
            "request": "launch",
            "program": "frontend/flaskmiddle/run.py",
            "jinja": true,
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: QUERIES Celery WINDOWS",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.worker.celery_start_queries",
                "worker",
                "--pool=threads",
                "--concurrency=8",
                "-Q",
                "fila_queries",
                "-l",
                "INFO",
                "-n",
                "worker1@%h",
                "--events"
            ],
            "jinja": true,
            "justMyCode": true,
            "gevent": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: QUERIES Celery LINUX",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.worker.celery_start_queries",
                "worker",
                "--pool=gevent",
                "-c",
                "4",
                "-Q",
                "fila_queries",
                "-l",
                "INFO",
                "-n",
                "worker1@%h"
            ],
            "jinja": true,
            "justMyCode": true,
            "gevent": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: SERVER PPG",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        {
            "name": "Python: SERVER PPGLS",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api_ppgls.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        {
            "name": "Python: SERVER GRAD",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api_grad.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        {
            "name": "Python: SERVER USUARIOS",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api_usuarios.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        // {
        //     "name": "Python: SERVER LOGIN",
        //     "type": "debugpy",
        //     "request": "launch",
        //     "program": "${workspaceFolder}/backend/app/api/api_login.py",
        //     "console": "integratedTerminal",
        //     "justMyCode": true,
        //     "envFile": "${workspaceFolder}/.env.desenvolvimento",
        //     "args": []
        // },
        {
            "name": "Python: API LOGIN",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": [
                "backend.app.api.api_login:app",
                "--reload",
                "--port",
                "8002"
            ],
            "jinja": true
        },
        {
            "name": "Next.js: debug full stack",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/frontend",
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "request": "launch",
            "command": "npm run dev",
            "serverReadyAction": {
                "pattern": "- Local:.+(https?://.+)",
                "uriFormat": "%s",
                "action": "debugWithChrome"
            }
        }
    ]
}