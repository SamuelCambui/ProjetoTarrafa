        {
            "name": "Python: QUERIES Celery WINDOWS",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "args": [
                "-A",
                "backend.worker.celery_start_queries",
                "worker",
                "--pool=threads",
                "--concurrency=8",
                "-Q",
                "fila_queries",
                "-l",
                "INFO",
                "-n",
                "worker1@%h"
            ],
            "jinja": true,
            "justMyCode": true,
            "gevent": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python: INDICADORES gRPC SERVERPPGLS",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/app/api/api_ppgls.py",  
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento",
            "args": []
        },
        {
            "name": "Next.js: debug full stack",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/frontend/frontend-next",
            "request": "launch",
            "command": "npm run dev",
            "serverReadyAction": {
              "pattern": "- Local:.+(https?://.+)",
              "uriFormat": "%s",
              "action": "debugWithChrome"
            }
          },
          {
            "name": "Python Debugger: Login FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "backend.app.api.api_login:app",
                "--reload",
                "--port",
                "8002"
            ],
            "jinja": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        },
        {
            "name": "Python Debugger: Login Formulario FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "backend.app.api.api_login_formulario:app",
                "--reload",
                "--port",
                "8003"
            ],
            "jinja": true,
            "envFile": "${workspaceFolder}/.env.desenvolvimento"
        }

      
    ]

}
